<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mf.core.mapper.OrderMapper">
    <resultMap id="orderMapper" type="com.mf.core.model.Order">
        <result column="id" jdbcType="BIGINT" property="id"/>
        <result column="order_no" jdbcType="BIGINT" property="orderNo"/>
        <result column="amount" jdbcType="INTEGER" property="amount"/>
        <result column="status" jdbcType="DOUBLE" property="status"/>
        <result column="user_id" jdbcType="BIGINT" property="userId"/>
        <result column="coupon_record_id" jdbcType="BIGINT" property="couponRecordId"/>
        <result column="create_time" jdbcType="TIMESTAMP" property="createTime"/>
        <result column="update_time" jdbcType="TIMESTAMP" property="updateTime"/>
    </resultMap>
    <update id="consumerOrder" parameterType="com.mf.core.model.Order">
        update `test`.t_order
        set status=#{status}
        where id=#{id}
    </update>
</mapper>
